{
  "private": true,
  "scripts": {
    "dev": "vite --port 3333 --open",
    "build": "vite-ssg build",
    "preview": "vite preview --open",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,vue}\"",
    "lint": "eslint --fix --ext .ts,.tsx,.js,.jsx,.vue src",
    "tc": "vue-tsc --noEmit",
    "prepare": "husky install",
    "postinstall": "patch-package --patch-dir .patches"
  },
  "dependencies": {
    "vue": "^3.2.21",
    "vue-router": "^4.0.12"
  },
  "devDependencies": {
    "@iconify/json": "^1.1.424",
    "@types/markdown-it-link-attributes": "^3.0.1",
    "@types/node": "^16.11.6",
    "@typescript-eslint/eslint-plugin": "^5.3.0",
    "@typescript-eslint/parser": "^5.3.0",
    "@vitejs/plugin-vue": "^1.9.4",
    "@vue/compiler-sfc": "^3.2.21",
    "@vue/server-renderer": "^3.2.21",
    "@vueuse/head": "^0.5.1",
    "eslint": "^8.1.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^7.0.4",
    "lint-staged": "^11.2.6",
    "markdown-it-link-attributes": "^3.0.0",
    "markdown-it-prism": "^2.2.1",
    "patch-package": "^6.4.7",
    "prettier": "^2.4.1",
    "prism-theme-vars": "^0.2.2",
    "typescript": "^4.4.4",
    "unplugin-icons": "^0.12.18",
    "unplugin-vue-components": "^0.17.0",
    "vite": "^2.6.13",
    "vite-plugin-md": "^0.11.4",
    "vite-plugin-pages": "^0.18.2",
    "vite-plugin-vue-layouts": "^0.5.0",
    "vite-plugin-windicss": "^1.5.1",
    "vite-ssg": "^0.15.6",
    "vue-tsc": "^0.28.10",
    "windicss": "^3.2.1"
  },
  "lint-staged": {
    "src/**/*.{ts,tsx,js,jsx,vue}": [
      "eslint --fix --max-warnings=0"
    ]
  }
}
